{
  "name": "@qingshaner/utility",
  "type": "module",
  "version": "0.2.0",
  "description": "Some utilities for JavaScript & TypeScript projects",
  "author": {
    "name": "qingshaner",
    "url": "https://github.com/Jon-a-than"
  },
  "license": "ISC",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "vite build"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "typescript": ">=5.5.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@qingshaner/utility-client": "workspace:*",
    "@qingshaner/utility-server": "workspace:*",
    "@qingshaner/utility-shared": "workspace:*"
  }
}
